{
  "name": "retropixels",
  "version": "0.6.5-snapshot",
  "description": "A cross platform command line tool to convert images to Commodore 64 format.",
  "main": "target/index.js",
  "bin": {
    "retropixels": "./cli.js"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest",
    "lint": "npx tslint --project .",
    "fix": "npx tslint --project . --fix"
  },
  "dependencies": {
    "commander": "5.0.0",
    "fs-extra": "9.0.0",
    "jimp": "0.10.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "8.1.0",
    "@types/node": "13.11.0",
    "ajv": "6.12.0",
    "c64jasm": "0.8.1",
    "del": "5.1.0",
    "jest": "25.2.7",
    "retire": "2.2.0",
    "ts-jest": "25.3.1",
    "tslint": "6.1.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micheldebree/retropixels.git"
  },
  "author": "Michel de Bree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micheldebree/retropixels/issues"
  },
  "homepage": "https://github.com/micheldebree/retropixels#readme"
}
