{
  "name": "retropixels",
  "version": "0.6.2",
  "description": "A cross platform command line tool to convert images to Commodore 64 format.",
  "main": "index.js",
  "bin": {
    "retropixels": "./index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest",
    "lint": "npx tslint --project .",
    "fix": "npx tslint --project . --fix"
  },
  "dependencies": {
    "commander": "2.20.0",
    "fs-extra": "7.0.1",
    "jimp": "0.6.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.5",
    "@types/node": "11.13.0",
    "ajv": "6.10.0",
    "del": "3.0.0",
    "jest": "24.7.1",
    "retire": "2.0.2",
    "ts-jest": "24.0.2",
    "tslint": "5.15.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micheldebree/retropixels.git"
  },
  "author": "Michel de Bree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micheldebree/retropixels/issues"
  },
  "homepage": "https://github.com/micheldebree/retropixels#readme"
}
